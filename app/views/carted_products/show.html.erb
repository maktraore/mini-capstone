 <%= form_tag "/carts/#{@cart.id}", method: :get do %>
    Quantity: <input type="text" name= "quantity" value = "<%= CartedProduct.where(product_id: @cart.product_id, user_id: current_user.id, status: "carted").count %>"
   <input type="hidden" name="product_id" value="<%= @cart.product_id %>">
    Add to Cart: <input type="submit" value="Add to Cart">
    <% end %>